{
  "name": "portfolio",
  "version": "0.0.0",
  "engine": {
    "node": "10.12.0",
    "npm": "6.4.1"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "serve": "ng serve --aot",
    "test": "ng test",
    "lint": "ng lint",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "e2e": "start-server-and-test serve http-get://localhost:4200 cy:open",
    "e2e:run": "start-server-and-test serve http-get://localhost:4200 cy:run",
    "e2e:ci": "start-server-and-test serve http-get://localhost:4200 cy:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec cypress/integration/features/**/* --reporter mocha-multi-reporters --reporter-options configFile=cypress/config.json > reports/e2e/cypress-report.log",
    "cy:ci": "cypress run --spec cypress/integration/features/**/* --record",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run portfolio:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "prestyleguide": "ng build --extract-css true --output-path kss/dist-style",
    "styleguide": "rimraf styleguide && kss --config kss/kss-config.json",
    "poststyleguide": "cp -r src/assets/ kss/dist-style/styles.css  kss/kss-styles.css kss/index.php styleguide/ && rimraf kss/dist-style",
    "lighthouse": "lighthouse http://localhost:4200 --view --output-path ./reports/lighthouse-report.html",
    "lighthouse:open": "start-server-and-test serve http-get://localhost:4200 lighthouse ",
    "postinstall": "ng build --aot --prod",
    "start": "node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/animations": "7.0.0",
    "@angular/cli": "7.0.2",
    "@angular/common": "7.0.0",
    "@angular/compiler": "7.0.0",
    "@angular/compiler-cli": "7.0.0",
    "@angular/core": "7.0.0",
    "@angular/forms": "7.0.0",
    "@angular/http": "7.0.0",
    "@angular/platform-browser": "7.0.0",
    "@angular/platform-browser-dynamic": "7.0.0",
    "@angular/platform-server": "^7.1.4",
    "@angular/pwa": "^0.11.4",
    "@angular/router": "7.0.0",
    "@angular/service-worker": "^7.1.4",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/entity": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@types/node": "10.12.0",
    "core-js": "2.5.5",
    "express": "^4.16.4",
    "ng-simple-slideshow": "^1.2.8",
    "ngx-pagination": "^3.2.1",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/language-service": "7.0.0",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@compodoc/compodoc": "^1.1.7",
    "@ngrx/schematics": "^6.1.2",
    "@types/chai": "4.1.3",
    "@types/cucumber": "4.0.4",
    "@types/express": "^4.16.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/puppeteer": "^1.9.0",
    "chai": "~4.2.0",
    "codelyzer": "~4.5.0",
    "cucumber": "^5.0.3",
    "cypress": "^3.1.4",
    "cypress-cucumber-preprocessor": "^1.9.1",
    "jasmine": "~3.2.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~1.3.1",
    "karma-junit-reporter": "^1.2.0",
    "kss": "3.0.0-beta.19",
    "michelangelo": "^0.7.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-multi-reporters": "^1.1.7",
    "ngrx-store-freeze": "^0.2.4",
    "protractor": "^5.4.1",
    "protractor-cucumber-framework": "^6.1.1",
    "puppeteer": "^1.9.0",
    "start-server-and-test": "^1.7.11",
    "ts-loader": "^5.3.2",
    "ts-node": "~7.0.0",
    "tsify": "^4.0.1",
    "tslint": "~5.11.0",
    "typescript": "3.1.3",
    "webpack-cli": "^3.1.2"
  },
  "env": {
    "NODE_ENV": "heroku"
  }
}
