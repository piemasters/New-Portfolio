  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "serve": "ng serve --aot",
    "test": "ng test",
    "lint": "ng lint",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e",
    "prestyleguide": "ng build --extract-css true --output-path kss/dist-style",
