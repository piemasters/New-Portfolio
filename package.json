{
  "name": "portfolio",
  "version": "0.0.0",
  "engine": {
    "node": "10.12.0",
    "npm": "6.4.1"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "serve": "ng serve --aot",
    "test": "ng test",
    "lint": "ng lint",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "e2e": "start-server-and-test serve http-get://localhost:4200 cy:open",
    "e2e:run": "start-server-and-test serve http-get://localhost:4200 cy:run",
    "e2e:ci": "start-server-and-test serve http-get://localhost:4200 cy:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec cypress/integration/features/**/* --reporter mocha-multi-reporters --reporter-options configFile=cypress/config.json > reports/e2e/cypress-report.log",
    "cy:ci": "cypress run --spec cypress/integration/features/**/* --record",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run portfolio:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "prestyleguide": "ng build --extract-css true --output-path kss/dist-style",
    "styleguide": "rimraf styleguide && kss --config kss/kss-config.json",
    "poststyleguide": "cp -r src/assets/ kss/dist-style/styles.css  kss/kss-styles.css kss/index.php styleguide/ && rimraf kss/dist-style",
    "lighthouse": "lighthouse http://localhost:4200 --view --output-path ./reports/lighthouse-report.html",
    "lighthouse:open": "start-server-and-test serve http-get://localhost:4200 lighthouse ",
    "stats": "webpack-bundle-analyzer dist/portfolio-browser/stats.json",
    "postinstall": "ng build --aot --prod",
    "start": "node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "0.13.3",
    "@angular/animations": "7.2.6",
    "@angular/cli": "7.3.3",
    "@angular/common": "7.2.6",
    "@angular/compiler": "7.2.6",
    "@angular/compiler-cli": "7.2.6",
    "@angular/core": "7.2.6",
    "@angular/forms": "7.2.6",
    "@angular/http": "7.2.6",
    "@angular/platform-browser": "7.2.6",
    "@angular/platform-browser-dynamic": "7.2.6",
    "@angular/platform-server": "7.2.6",
    "@angular/pwa": "0.13.3",
    "@angular/router": "7.2.6",
    "@angular/service-worker": "7.2.6",
    "@ngrx/effects": "7.2.0",
    "@ngrx/entity": "7.2.0",
    "@ngrx/router-store": "7.2.0",
    "@ngrx/store": "7.2.0",
    "@ngrx/store-devtools": "7.2.0",
    "@nguniversal/module-map-ngfactory-loader": "7.1.0",
    "@types/node": "11.9.5",
    "core-js": "2.6.5",
    "express": "4.16.4",
    "ngx-lightbox": "^1.2.0",
    "ngx-pagination": "3.2.1",
    "rxjs": "~6.4.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular/language-service": "7.2.6",
    "@bahmutov/add-typescript-to-cypress": "2.0.0",
    "@compodoc/compodoc": "1.1.8",
    "@ngrx/schematics": "7.2.0",
    "@types/chai": "4.1.7",
    "@types/cucumber": "4.0.5",
    "@types/express": "4.16.1",
    "@types/jasmine": "3.3.9",
    "@types/jasminewd2": "2.0.6",
    "@types/puppeteer": "1.12.1",
    "chai": "4.2.0",
    "codelyzer": "4.5.0",
    "cucumber": "5.1.0",
    "cypress": "3.1.5",
    "cypress-cucumber-preprocessor": "1.11.0",
    "jasmine": "3.3.1",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-junit-reporter": "1.2.0",
    "kss": "3.0.0-beta.25",
    "michelangelo": "0.8.0",
    "mocha": "6.0.2",
    "mocha-junit-reporter": "1.18.0",
    "mocha-multi-reporters": "1.1.7",
    "ngrx-store-freeze": "0.2.4",
    "protractor": "5.4.2",
    "protractor-cucumber-framework": "6.1.1",
    "puppeteer": "1.12.2",
    "source-map-explorer": "^1.7.0",
    "start-server-and-test": "1.7.11",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.2",
    "tsify": "4.0.1",
    "tslint": "5.13.0",
    "typescript": "3.2.4",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "3.2.3"
  },
  "env": {
    "NODE_ENV": "heroku"
  }
}
